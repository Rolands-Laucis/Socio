  },
  "homepage": "https://github.com/Rolands-Laucis/Socio#readme",
  "dependencies": {
    "js-yaml": "^4.1.0",
    "magic-string": "^0.30.5",
    "pako": "^2.1.0",
    "recursive-diff": "^1.0.9",
    "ws": "^8.9.0"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^18.15.0",
    "@types/pako": "^2.0.3",
    "@types/ws": "^8.5.3"
