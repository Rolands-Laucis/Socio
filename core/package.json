{
  "name": "socio",
  "version": "1.8.3",
  "description": "A WebSocket Real-Time Communication (RTC) API framework.",
  "main": "./dist/core.js",
  "type": "module",
  "module": "core.ts",
  "scripts": {
    "build": "tsc",
    "pub": "npm run build && ./copy.ps1 && npm publish",
